{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "rosewater": "#f4dbd6",
        "flamingo": "#f0c6c6",
        "pink": "#f5bde6",
        "mauve": "#c6a0f6",
        "red": "#ed8796",
        "maroon": "#ee99a0",
        "peach": "#f5a97f",
        "yellow": "#eed49f",
        "green": "#a6da95",
        "teal": "#8bd5ca",
        "sky": "#91d7e3",
        "sapphire": "#7dc4e4",
        "blue": "#8aadf4",
        "lavender": "#b7bdf8",
        "text": "#cad3f5",
        "subtext1": "#b8c0e0",
        "subtext0": "#a5adcb",
        "overlay2": "#939ab7",
        "overlay1": "#8087a2",
        "overlay0": "#6e738d",
        "surface2": "#5b6078",
        "surface1": "#494d64",
        "surface0": "#363a4f",
        "base": "#24273a",
        "mantle": "#1e2030",
        "crust": "#181926"
    },
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "trailing_diamond": "",
            "segments": [
                {
                    "type": "path",
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b4",
                    "background": "p:base",
                    "foreground": "p:text",
                    "template": "\ue5ff {{ .Path }}",
                    "properties": {
                        "max_depth": 3,
                        "style": "agnoster_short"
                    }
                },
                {
                    "type": "git",
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b4",
                    "background": "p:base",
                    "foreground": "p:sapphire",
                    "template": "{{ .UpstreamIcon }}{{ .HEAD }}",
                    "properties": {
                        "fetch_upstream_icon": true,
                        "branch_icon": "",
                        "commit_icon": "@"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "python",
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b4",
                    "background": "p:base",
                    "foreground": "p:yellow",
                    "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }} venv {{ end }}{{ .Full }}{{ end }}"
                },
                {
                    "type": "node",
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b4",
                    "background": "p:base",
                    "foreground": "p:green",
                    "template": "{{ if .Full }} {{ .Full }}{{ else }}  {{ end }}"
                },
                {
                    "type": "react",
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b4",
                    "background": "p:base",
                    "foreground": "p:blue",

                    "template": "{{ if .Full }} {{ .Full }}{{ else }}  {{ end }}"
                },
                {
                    "type": "cmake",
                    "style": "diamond",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b4",
                    "background": "p:base",
                    "foreground": "p:blue",

                    "template": "{{ if .Full }}  cmake {{ .Full }}{{ else }}   cmake {{ end }}"
                }
            ]
        },
        {
            "type": "rprompt",
            "overflow": "hidden",
            "segments": [
                {
                    "type": "executiontime",
                    "style": "plain",
                    "background": "transparent",
                    "foreground": "p:yellow",
                    "template": "{{ .FormattedMs }}",
                    "properties": {
                        "threshold": 0
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "\ue0b4",
                    "background": "transparent",
                    "foreground": "p:green",
                    "template": "  "
                }
            ]
        }
    ],
    "transient_prompt": {
        "background": "transparent",
        "foreground": "p:text",
        "template": "   "
    },
    "final_space": true,
    "console_title_template": "{{ .PWD }} - Terminal",
    "version": 2
}
